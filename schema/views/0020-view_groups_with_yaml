/*
** Consolidate group/metadata information into one row per group/cloud.
*/
create or replace view view_groups_with_metadata as

select

cg.group_name,
condor_central_manager,
metadata_name,
enabled,
priority,
metadata,
mime_type

from csv2_groups as cg
left outer join csv2_group_metadata as cgm on cg.group_name=cgm.group_name

;
