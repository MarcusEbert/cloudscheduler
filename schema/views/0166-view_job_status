/*
** Retrieve groups/cloud summary information.
*/
create or replace view view_job_status as

/*
** Retrieve info.
*/
select

vjsr.group_name,
sum(Jobs) as Jobs,
sum(Idle) as Idle,
sum(Running) as Running,
sum(Completed) as Completed,
sum(Held) as Held,
sum(Other) as Other

from view_job_status_raw as vjsr

group by group_name

;
